{
    "trailingSlash": "auto",
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": ["/*.{html,js,css,ico,png,svg,webp,webmanifest}"]
    },
    "globalHeaders": {
        "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
    },
    "mimeTypes": {
        ".json": "text/json"
    }
}
