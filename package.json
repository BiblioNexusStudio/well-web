{
    "name": "aquifer-web",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "test": "vitest",
        "lint:unused-translations": "( eslint --max-warnings 0 --rule \"svelte-translate-check/unused-translations:error\" \"src/lib/i18n/locales/*.json\" || rm -f tmp/all-translate-calls.txt ) || exit 1",
        "lint": "yarn format && eslint --max-warnings 0 --fix . && yarn lint:unused-translations && npx svelte-check",
        "lint:ci": "prettier --plugin-search-dir . --check \"**/*.{json,js,ts,css,html,svelte,cjs}\" && eslint --max-warnings 0 . && yarn lint:unused-translations && yarn svelte-check",
        "format": "prettier --plugin-search-dir . --write \"**/*.{json,js,ts,css,html,svelte,cjs}\"",
        "use-config": "node scripts/use-config.js",
        "postinstall": "patch-package"
    },
    "devDependencies": {
        "@capacitor/android": "^5.3.0",
        "@capacitor/assets": "^2.0.4",
        "@capacitor/cli": "^5.3.0",
        "@capacitor/core": "^5.3.0",
        "@sveltejs/adapter-auto": "^2.0.0",
        "@sveltejs/adapter-static": "^2.0.2",
        "@sveltejs/kit": "^1.20.4",
        "@tailwindcss/typography": "^0.5.9",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/svelte": "^4.0.3",
        "@types/howler": "^2.2.7",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "@vite-pwa/sveltekit": "^0.2.5",
        "async-es": "^3.2.4",
        "autoprefixer": "^10.4.14",
        "daisyui": "^3.2.1",
        "dotenv": "^16.3.1",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-svelte": "^2.30.0",
        "eslint-plugin-svelte-translate-check": "https://github.com/BiblioNexusStudio/eslint-plugin-svelte-translate-check",
        "jsdom": "^22.1.0",
        "patch-package": "^8.0.0",
        "postcss": "^8.4.25",
        "postinstall-postinstall": "^2.1.0",
        "prettier": "^2.8.0",
        "prettier-plugin-svelte": "^2.10.1",
        "svelte": "^4.0.0",
        "svelte-awesome": "^3.2.1",
        "svelte-check": "^3.5.0",
        "svelte-meta-tags": "^3.0.3",
        "tailwindcss": "^3.3.2",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "vite": "^4.3.6",
        "vite-plugin-pwa": "^0.16.4",
        "vitest": "^0.32.2",
        "workbox-window": "^7.0.0"
    },
    "type": "module",
    "dependencies": {
        "@microsoft/applicationinsights-web": "^3.0.2",
        "howler": "^2.2.3",
        "svelte-i18n": "^3.7.0"
    }
}
