{
    "name": "well-web",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "test": "vitest",
        "lint:unused-translations": "( eslint --max-warnings 0 --rule \"svelte-translate-check/unused-translations:error\" \"src/lib/i18n/locales/*.json\" || rm -f tmp/all-translate-calls.txt ) || exit 1",
        "lint": "yarn format && eslint --max-warnings 0 --fix . && yarn lint:unused-translations && npx svelte-check",
        "lint:ci": "prettier --check \"**/*.{json,js,ts,css,html,svelte,cjs}\" && eslint --max-warnings 0 . && yarn lint:unused-translations && yarn svelte-check",
        "format": "prettier --write \"**/*.{json,js,ts,css,html,svelte,cjs}\"",
        "use-config": "node scripts/use-config.js",
        "postinstall": "patch-package",
        "apk:build": "node ./scripts/build-android-apk.js",
        "apk:preload-content": "node ./scripts/preload-files-to-static.js",
        "prepare": "husky install"
    },
    "dependencies": {
        "@microsoft/applicationinsights-web": "^3.0.3",
        "@sveltejs/kit": "^1.25.2",
        "@tiptap/core": "^2.1.12",
        "@tiptap/extension-highlight": "^2.1.12",
        "@tiptap/extension-image": "^2.1.12",
        "@tiptap/extension-link": "^2.1.12",
        "@tiptap/extension-subscript": "^2.1.12",
        "@tiptap/extension-superscript": "^2.1.12",
        "@tiptap/extension-text-style": "^2.1.12",
        "@tiptap/extension-underline": "^2.1.12",
        "@tiptap/html": "^2.1.12",
        "@tiptap/pm": "^2.1.12",
        "@tiptap/starter-kit": "^2.1.12",
        "@vite-pwa/sveltekit": "^0.2.7",
        "async-es": "^3.2.4",
        "cupertino-pane": "^1.4.2",
        "daisyui": "^4.5.0",
        "localforage": "^1.10.0",
        "svelte": "^4.2.1",
        "svelte-awesome": "^3.2.1",
        "svelte-i18n": "3.7.3",
        "svelte-meta-tags": "^3.0.4",
        "unzipit": "^1.4.3"
    },
    "devDependencies": {
        "@capacitor/android": "^5.5.0",
        "@capacitor/assets": "^3.0.1",
        "@capacitor/cli": "^5.5.0",
        "@capacitor/core": "^5.5.0",
        "@sveltejs/adapter-auto": "^2.0.0",
        "@sveltejs/adapter-static": "^2.0.3",
        "@tailwindcss/typography": "^0.5.10",
        "@testing-library/jest-dom": "^6.1.4",
        "@testing-library/svelte": "^4.0.3",
        "@typescript-eslint/eslint-plugin": "^6.7.5",
        "@typescript-eslint/parser": "^6.7.5",
        "autoprefixer": "^10.4.17",
        "dotenv": "^16.3.1",
        "eslint": "^8.51.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-svelte": "^2.34.0",
        "eslint-plugin-svelte-translate-check": "https://github.com/BiblioNexusStudio/eslint-plugin-svelte-translate-check",
        "husky": "^8.0.3",
        "jsdom": "^22.1.0",
        "kolorist": "^1.8.0",
        "patch-package": "^8.0.0",
        "postcss": "^8.4.33",
        "postinstall-postinstall": "^2.1.0",
        "prettier": "^3.0.3",
        "prettier-plugin-svelte": "^3.0.3",
        "prettier-plugin-tailwindcss": "^0.5.5",
        "svelte-check": "^3.5.2",
        "tailwindcss": "^3.4.1",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2",
        "vite": "^4.5.2",
        "vite-plugin-pwa": "^0.16.5",
        "vitest": "^0.34.6",
        "workbox-background-sync": "^7.0.0",
        "workbox-build": "^7.0.0",
        "workbox-cacheable-response": "^7.0.0",
        "workbox-core": "^7.0.0",
        "workbox-precaching": "^7.0.0",
        "workbox-range-requests": "^7.0.0",
        "workbox-routing": "^7.0.0",
        "workbox-strategies": "^7.0.0"
    },
    "type": "module"
}
